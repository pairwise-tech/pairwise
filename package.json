{
  "name": "prototype-x",
  "version": "0.1.0",
  "description": "Prototype X",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "jest": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --list-different './src/**/*.{ts,tsx,json}'",
    "prettier:fix": "prettier --write './src/**/*.{ts,tsx,json}'",
    "tslint": "tslint --project tsconfig.json -c tslint.json",
    "tslint:fix": "tslint --project tsconfig.json -c tslint.json --fix",
    "format": "yarn prettier:fix && yarn tslint:fix",
    "tsc": "tsc",
    "test:watch": "yarn jest",
    "test:unit": "yarn jest --watchAll=false",
    "test:update-snapshots": "yarn jest --updateSnapshot",
    "lint": "yarn prettier && yarn tslint && yarn tsc",
    "test": "yarn lint && yarn test:unit",
    "deploy": "yarn test && yarn build"
  },
  "dependencies": {
    "@babel/standalone": "^7.7.4",
    "@monaco-editor/react": "^2.3.0",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.12",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/styled-components": "^4.4.0",
    "@types/throttle-debounce": "^2.1.0",
    "axios": "^0.19.0",
    "console-feed": "^2.8.10",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-grid-resizable": "^1.0.0",
    "react-scripts": "3.2.0",
    "styled-components": "^4.4.1",
    "throttle-debounce": "^2.1.0",
    "tslint": "^5.20.1",
    "tslint-immutable": "^6.0.1",
    "typescript": "3.7.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
