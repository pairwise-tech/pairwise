{
  "name": "e2e",
  "version": "0.0.1",
  "description": "e2e test suites for Prototype X",
  "private": true,
  "main": "dist/e2e.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "mock:build": "tsc --lib esnext --esModuleInterop --skipLibCheck mock-server/*.ts",
    "mock:watch": "concurrently \"yarn run mock:build -- -w\" \"nodemon --inspect ./mock-server/server.js\"",
    "mock:start": "node ./mock-server/server.js",
    "prettier": "prettier --list-different \"src/**/*.ts\" \"test/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "tslint": "tslint --project tsconfig.json -c tslint.json",
    "tslint:fix": "tslint --project tsconfig.json -c tslint.json --fix",
    "format": "yarn prettier:fix && yarn tslint:fix",
    "lint": "yarn prettier && yarn tslint",
    "jest": "jest --config ./jest-config.json",
    "test:unit": "jest",
    "test:watch": "yarn jest --watch",
    "test:cov": "yarn jest --coverage",
    "test": "yarn lint",
    "test:e2e": "yarn jest"
  },
  "directories": {
    "dist": "dist"
  },
  "typings": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/faker": "^4.1.8",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "morgan": "^1.9.1"
  }
}
