{
  "name": "@pairwise/e2e",
  "version": "0.0.1",
  "description": "Pairwise e2e test suite",
  "private": true,
  "main": "dist/e2e.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prettier": "prettier --list-different \"src/**/*.ts\" \"src/**/*.test.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\" \"src/**/*.test.ts\"",
    "eslint": "eslint 'src/**/*.{ts,tsx}'",
    "eslint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "yarn prettier:fix && yarn eslint:fix",
    "lint": "yarn prettier && yarn eslint",
    "test": "yarn lint",
    "test:e2e": "yarn jest --config ./jest-config.json",
    "test:watch": "yarn test:e2e --watch",
    "test:debug": "yarn test:e2e --watch -f src/user.e2e.test.ts"
  },
  "directories": {
    "dist": "dist"
  },
  "typings": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/jest": "26.0.19",
    "axios": "0.19.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.8",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "faker": "4.1.0",
    "jest": "26.6.3",
    "prettier": "2.3.1",
    "supertest": "4.0.2",
    "typescript": "4.3.4"
  }
}
