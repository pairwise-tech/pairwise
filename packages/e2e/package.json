{
  "name": "@pairwise/e2e",
  "version": "0.0.1",
  "description": "Pairwise e2e test suite",
  "private": true,
  "main": "dist/e2e.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prettier": "prettier --list-different \"src/**/*.ts\" \"test/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "eslint": "eslint 'src/**/*.{ts,tsx}'",
    "eslint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "yarn prettier:fix && yarn tslint:fix",
    "lint": "yarn prettier && yarn eslint",
    "test": "yarn lint",
    "test:e2e": "yarn jest --config ./jest-config.json",
    "test:watch": "yarn test:e2e --watch",
    "test:debug": "yarn test:e2e --watch -f src/user.e2e.test.ts"
  },
  "directories": {
    "dist": "dist"
  },
  "typings": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "axios": "0.19.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "faker": "4.1.0",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "typescript": "3.7.4"
  }
}
