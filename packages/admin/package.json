{
  "name": "@pairwise/admin",
  "version": "0.0.1",
  "description": "Pairwise admin dashboard",
  "private": true,
  "scripts": {
    "setup:env": "cp sample.env .env",
    "start": "PORT=3007 react-scripts start",
    "start:dev": "REACT_APP_DEV=true yarn run start",
    "build:common": "cd ../common && yarn run build",
    "start:https": "REACT_APP_HOST=https://localhost:9000 HTTPS=true yarn run start",
    "start:prod": "serve build -s -p 3000",
    "build": "react-scripts build",
    "postbuild": "cp _redirects build/",
    "jest": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --list-different './src/**/*.{ts,tsx,json}'",
    "prettier:fix": "prettier --write './src/**/*.{ts,tsx,json}'",
    "eslint": "eslint 'src/**/*.{ts,tsx}'",
    "eslint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint": "yarn prettier && yarn eslint && yarn tsc",
    "format": "yarn prettier:fix && yarn eslint:fix",
    "tsc": "tsc",
    "test:watch": "yarn jest",
    "test:unit": "yarn jest --watchAll=false",
    "test:update-snapshots": "yarn jest --updateSnapshot",
    "test": "yarn lint && yarn test:unit"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.7.5",
    "@babel/preset-es2015": "7.0.0-beta.53",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@blueprintjs/core": "3.46.0",
    "@blueprintjs/popover2": "0.11.0",
    "@blueprintjs/select": "3.16.5",
    "@types/classnames": "2.2.9",
    "@types/getenv": "1.0.0",
    "@types/jest": "26.0.19",
    "@types/md5": "2.2.0",
    "@types/prettier": "1.19.0",
    "@types/ramda": "0.27.41",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/react-helmet": "6.1.0",
    "@types/react-json-pretty": "2.2.0",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.3",
    "@types/react-transition-group": "4.4.0",
    "@types/redux-logger": "3.0.8",
    "@types/shortid": "0.0.29",
    "@types/smoothscroll-polyfill": "0.3.1",
    "@types/styled-components": "5.1.10",
    "@types/throttle-debounce": "2.1.0",
    "@types/traverse": "0.6.32",
    "@types/uuid": "8.3.0",
    "array-move": "2.2.1",
    "axios": "0.21.1",
    "date-fns": "2.14.0",
    "email-validator": "2.0.4",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.8",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "getenv": "1.0.0",
    "history": "4.10.1",
    "is-mobile": "2.2.1",
    "md5": "2.3.0",
    "node-sass": "4.14.1",
    "prettier": "2.3.1",
    "query-string": "6.9.0",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-device-detect": "1.15.0",
    "react-diff-viewer": "3.1.1",
    "react-dom": "17.0.2",
    "react-json-pretty": "2.2.0",
    "react-markdown": "5.0.3",
    "react-redux": "7.2.4",
    "react-router-dom": "5.1.2",
    "react-scripts": "4.0.3",
    "react-string-replace": "0.4.4",
    "react-transition-group": "4.3.0",
    "redux": "4.1.0",
    "redux-devtools-extension": "2.13.9",
    "redux-logger": "3.0.6",
