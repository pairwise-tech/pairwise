{
  "name": "@pairwise/admin",
  "version": "0.0.1",
  "description": "Pairwise Admin UI",
  "private": true,
  "scripts": {
    "setup:env": "cp sample.env .env",
    "start": "react-scripts start",
    "build:common": "cd ../common && yarn run build",
    "start:https": "REACT_APP_HOST=https://localhost:9000 HTTPS=true yarn run start",
    "start:prod": "serve build -s -p 3000",
    "build": "react-scripts build",
    "postbuild": "cp _redirects build/",
    "jest": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --list-different './src/**/*.{ts,tsx,json}'",
    "prettier:fix": "prettier --write './src/**/*.{ts,tsx,json}'",
    "tslint": "tslint --project tsconfig.json -c tslint.json",
    "tslint:fix": "tslint --project tsconfig.json -c tslint.json --fix",
    "format": "yarn prettier:fix && yarn tslint:fix",
    "tsc": "tsc",
    "test:watch": "yarn jest",
    "test:unit": "yarn jest --watchAll=false",
    "test:update-snapshots": "yarn jest --updateSnapshot",
    "lint": "yarn prettier && yarn tslint && yarn tsc",
    "test": "yarn lint && yarn test:unit"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.7.5",
    "@babel/preset-es2015": "7.0.0-beta.53",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@blueprintjs/core": "3.22.3",
    "@blueprintjs/select": "3.11.2",
    "@types/classnames": "2.2.9",
    "@types/getenv": "1.0.0",
    "@types/jest": "25.1.1",
    "@types/md5": "2.2.0",
    "@types/prettier": "1.19.0",
    "@types/ramda": "0.26.38",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "6.1.0",
    "@types/react-redux": "7.1.5",
    "@types/react-router-dom": "5.1.3",
    "@types/react-transition-group": "4.4.0",
    "@types/redux-logger": "3.0.7",
    "@types/shortid": "0.0.29",
    "@types/smoothscroll-polyfill": "0.3.1",
    "@types/styled-components": "4.4.1",
    "@types/throttle-debounce": "2.1.0",
    "@types/traverse": "0.6.32",
    "@types/uuid": "^8.3.0",
    "array-move": "2.2.1",
    "axios": "0.19.0",
    "date-fns": "2.14.0",
    "debug": "4.1.1",
    "email-validator": "2.0.4",
    "getenv": "1.0.0",
    "history": "4.10.1",
    "is-mobile": "2.2.1",
    "md5": "2.3.0",
    "node-sass": "4.14.1",
    "prettier": "1.19.1",
    "query-string": "6.9.0",
    "ramda": "0.26.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.3.0",
    "react-string-replace": "0.4.4",
    "react-transition-group": "4.3.0",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-logger": "3.0.6",
    "redux-observable": "1.2.0",
    "reselect": "4.0.0",
    "rxjs": "6.5.3",
    "serve": "11.3.0",
    "shortid": "2.2.15",
    "smoothscroll-polyfill": "0.4.4",
    "string-quote-x": "3.1.2",
    "strip-comments": "2.0.1",
    "styled-components": "4.4.1",
    "swipyjs": "0.0.5",
    "throttle-debounce": "2.1.0",
    "traverse": "0.6.6",
    "truncate": "2.1.0",
    "ts-morph": "5.0.0",
    "tslint": "5.20.1",
    "typesafe-actions": "5.1.0",
    "typescript": "3.7.2",
    "use-media": "1.4.0",
    "uuid": "^8.3.1",
    "workerize-loader": "1.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:9000"
}
