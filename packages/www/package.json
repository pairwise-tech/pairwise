{
  "name": "@pairwise/www",
  "version": "0.0.1",
  "description": "The Pairwise Website",
  "author": "Pairwise Team",
  "dependencies": {
    "@material-ui/core": "4.8.3",
    "@material-ui/icons": "4.5.1",
    "add": "2.0.6",
    "gatsby": "2.19.21",
    "gatsby-image": "2.2.38",
    "gatsby-plugin-amplitude-analytics": "2.0.0",
    "gatsby-plugin-google-analytics": "2.1.36",
    "gatsby-plugin-manifest": "2.2.37",
    "gatsby-plugin-material-ui": "2.1.6",
    "gatsby-plugin-offline": "2.2.10",
    "gatsby-plugin-react-helmet": "3.1.21",
    "gatsby-plugin-sharp": "2.4.5",
    "gatsby-plugin-styled-components": "3.1.17",
    "gatsby-plugin-typescript": "2.1.26",
    "gatsby-source-filesystem": "2.1.46",
    "gatsby-source-graphql": "2.1.31",
    "gatsby-transformer-sharp": "2.3.12",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "normalize.css": "8.0.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-markdown": "4.2.2",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.11.2",
    "@graphql-codegen/introspection": "1.11.2",
    "@graphql-codegen/typescript": "1.11.2",
    "@graphql-codegen/typescript-operations": "1.11.2",
    "@types/graphql": "14.5.0",
    "@types/node": "12.12.24",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "5.0.15",
    "@types/styled-components": "4.4.2",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-plugin-styled-components": "1.10.6",
    "concurrently": "4.1.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-node": "9.2.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-standard": "4.0.1",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "typescript": "3.7.4",
    "typescript-styled-plugin": "0.14.0"
  },
  "scripts": {
    "build": "gatsby build",
    "compress-media-kit": "cd src/images && zip -9 -r media-kit.zip media-kit/",
    "develop": "yarn run start",
    "start:dev": "yarn run start",
    "format": "prettier --write '{__mocks__,src,test}/**/*.{js,ts,tsx}'",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "lint:fix": "yarn run lint --fix",
    "start": "gatsby develop",
    "start:prod": "gatsby serve",
    "serve": "yarn run start:prod",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "codegen": "graphql-codegen --config graphql-codegen.yml"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ],
    "*.{md,yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.svg": [
      "svgo --pretty --indent=2 --config=svgo.yml --multipass",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "keywords": [
    "gatsby",
    "typescript",
    "graphql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spawnia/gatsby-starter-typescript-graphql"
  }
}
