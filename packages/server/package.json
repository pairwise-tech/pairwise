{
  "name": "@pairwise/server",
  "version": "0.0.1",
  "description": "Pairwise NestJS Server Application",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:watch": "nest build --watch",
    "setup:env": "cp sample.env .env",
    "setup:db": "yarn build && yarn migration:run",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:https": "CLIENT_URL=https://127.0.0.1:3000 SERVER_HOST_URL=https://localhost:9000 HTTPS=true nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn migration:run && node dist/main.js",
    "prettier": "prettier --list-different \"src/**/*.ts\" \"src/**/*.spec.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\" \"src/**/*.spec.ts\"",
    "eslint": "eslint 'src/**/*.{ts,tsx}'",
    "eslint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "yarn prettier:fix && yarn eslint:fix",
    "lint": "yarn prettier && yarn eslint",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test": "yarn lint && yarn test:unit",
    "typeorm": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm migration:generate -n",
    "migration:create": "yarn typeorm migration:create -n",
    "migration:run": "yarn typeorm migration:run",
    "migration:show": "yarn typeorm migration:show",
    "migration:revert": "yarn typeorm migration:revert",
    "schema:drop": "yarn typeorm schema:drop",
    "schema:sync": "yarn typeorm schema:sync",
    "schema:log": "yarn typeorm schema:log",
    "migrations:reset": "yarn schema:drop && rm -rf src/migrations && mkdir src/migrations",
    "db:reset": "yarn schema:drop && yarn setup:db",
    "db": "docker-compose up",
    "edit:email": "open -a \"Google Chrome\" src/email/templates/index.html"
  },
  "dependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.18",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/swagger": "^4.8.1",
    "@nestjs/typeorm": "^7.1.5",
    "@sentry/node": "5.12.2",
    "@slack/web-api": "5.7.0",
    "@types/morgan": "1.7.37",
    "@types/validator": "12.0.1",
    "class-transformer": "0.3.1",
    "class-validator": "0.11.0",
    "clone-buffer": "1.0.0",
    "compression": "1.7.4",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.8",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "getenv": "1.0.0",
    "morgan": "1.9.1",
    "nodemailer": "6.4.6",
    "passport": "0.4.1",
    "passport-facebook": "3.0.0",
    "passport-github": "1.1.0",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.0",
    "pg": "7.15.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "rxjs": "6.5.3",
    "shortid": "2.2.16",
    "stripe": "8.17.0",
    "swagger-ui-express": "4.1.2",
    "ts-node": "9.1.1",
    "typeorm": "0.2.34",
    "typescript": "4.1.3",
    "validator": "12.1.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "6.7.6",
    "@nestjs/testing": "6.10.12",
    "@types/express": "4.17.2",
    "@types/jest": "26.0.19",
    "@types/node": "12.12.22",
    "@types/passport-github": "1.1.5",
    "@types/passport-jwt": "3.0.3",
    "jest": "26.6.0",
    "prettier": "2.3.1",
    "ts-jest": "26.4.4",
    "ts-loader": "6.2.1",
    "tsconfig-paths": "3.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
