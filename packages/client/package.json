{
  "name": "@prototype/client",
  "version": "0.0.1",
  "description": "Prototype X Client Project: Create React App Application",
  "private": true,
  "scripts": {
    "codepress:build": "tsc --lib esnext --esModuleInterop --skipLibCheck codepress/src/*.ts",
    "codepress:watch": "concurrently \"yarn run codepress:build -- -w\" \"nodemon --inspect ./codepress/src/index.js\"",
    "start": "react-scripts start",
    "start:codepress": "concurrently \"yarn run codepress:watch\" \"REACT_APP_HIDE_EMBEDS=true REACT_APP_DEV=true HTTPS=false react-scripts start\"",
    "start:dev": "REACT_APP_HIDE_EMBEDS=0 REACT_APP_DEV=true react-scripts start",
    "start:https": "HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "postbuild": "cp _redirects build/",
    "jest": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --list-different './src/**/*.{ts,tsx,json}'",
    "prettier:fix": "prettier --write './src/**/*.{ts,tsx,json}'",
    "tslint": "tslint --project tsconfig.json -c tslint.json",
    "tslint:fix": "tslint --project tsconfig.json -c tslint.json --fix",
    "format": "yarn prettier:fix && yarn tslint:fix",
    "tsc": "tsc",
    "test:watch": "yarn jest",
    "test:unit": "yarn jest --watchAll=false",
    "test:update-snapshots": "yarn jest --updateSnapshot",
    "lint": "yarn prettier && yarn tslint && yarn tsc",
    "test": "yarn lint && yarn test:unit"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/standalone": "^7.7.4",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@monaco-editor/react": "^2.3.0",
    "@types/getenv": "^1.0.0",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.12",
    "@types/ramda": "^0.26.36",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/react-facebook-login": "^4.1.0",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/shortid": "^0.0.29",
    "@types/styled-components": "^4.4.0",
    "@types/throttle-debounce": "^2.1.0",
    "axios": "^0.19.0",
    "concurrently": "^5.0.2",
    "console-feed": "^2.8.10",
    "debug": "^4.1.1",
    "getenv": "^1.0.0",
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "query-string": "^6.9.0",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.0.7",
    "react-grid-resizable": "^1.0.0",
    "react-markdown": "^4.2.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-social-login-buttons": "^3.0.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.2.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.3",
    "shortid": "^2.2.15",
    "styled-components": "^4.4.1",
    "throttle-debounce": "^2.1.0",
    "ts-morph": "^5.0.0",
    "tslint": "^5.20.1",
    "typesafe-actions": "^5.1.0",
    "typescript": "3.7.2",
    "workerize-loader": "^1.1.0"
  },
  "devDependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lerna": "^3.19.0",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn format"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:9000"
}
