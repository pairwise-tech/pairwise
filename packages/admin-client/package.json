{
  "name": "admin-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "sc": "shadow-cljs",
    "server": "shadow-cljs stop && shadow-cljs start --config-merge \"{:closure-defines {app.core/ADMIN_TOKEN $ADMIN_TOKEN}}\"",
    "prestart": "yarn",
    "prebuild": "yarn",
    "start": "export ADMIN_TOKEN=$ADMIN_TOKEN && shadow-cljs watch app --config-merge \"{:closure-defines {app.core/ADMIN_TOKEN $ADMIN_TOKEN}}\"",
    "cards": "shadow-cljs watch cards",
    "build": "yarn clean && shadow-cljs release app",
    "test": "shadow-cljs compile test && node out/test.js",
    "test:watch": "shadow-cljs watch test --config-merge \"{:autorun true}\"",
    "e2e": "shadow-cljs compile e2e && node out/e2e.js",
    "report": "shadow-cljs run shadow.cljs.build-report app report.html",
    "lint": "clj-kondo --lint client",
    "format": "zprint-clj --hang -i \"./client/**/*.{clj,cljs,cljc,edn}\" -o ./ && zprint-clj -i \"./*.edn\" -o ./",
    "clean": "rimraf public/js"
  },
  "devDependencies": {
    "@testing-library/dom": "^6.11.0",
    "@testing-library/react": "^9.4.0",
    "clj-kondo": "2020.01.13",
    "highlight.js": "9.18.0",
    "jsdom": "^16.0.1",
    "jsdom-global": "^3.0.2",
    "marked": "^0.8.0",
    "rimraf": "~3.0.0",
    "serve-handler": "~6.1.2",
    "shadow-cljs": "~2.8.83",
    "taiko": "~1.0.3",
    "taiko-diagnostics": "~0.4.0",
    "zprint-clj": "~0.8.0"
  },
  "dependencies": {
    "@blueprintjs/core": "3.22.3",
    "axios": "^0.20.0",
    "create-react-class": "~15.6.3",
    "react": "~16.8.0",
    "react-dom": "~16.8.0"
  }
}
